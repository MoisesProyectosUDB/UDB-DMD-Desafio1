                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select  IDVisita, case when NameTipoRosa='Rosas' then 1  
					   when NameTipoRosa='Claveles' then 2
					   when NameTipoRosa='Macetas' then 3
					   when NameTipoRosa='Tierra' then 4
             when NameTipoRosa='Girasoles' then 5
             when NameTipoRosa='Hortensia' then 6
             when NameTipoRosa='Globos' then 7
             when NameTipoRosa='Tarjetas' then 8
             when NameTipoRosa='Orquideas' then 9
             when NameTipoRosa='Carmensa' then 10
             when NameTipoRosa='Lirios' then 11
             when NameTipoRosa='Aurora' then 12
             when NameTipoRosa='Tulipanes' then 13
             when NameTipoRosa='Liston' then 14
					   else null
				  end as 'IDTipoRosa'
				  ,RosaPedido from (select d.ID as 'IDVisita',a.Rosas, a.Claveles, a.Macetas, a.Tierra, a.Girasoles, a.Hortensia, a.Globos, a.Tarjetas, a.Orquideas, a.Carmensa, a.Lirios, a.Aurora, a.Tulipanes, a.Liston
							 from DestinoDatos a inner join Sucursal b on a.Departamento =b.ID
							 inner join Clientes c on a.nombreCliente=c.nombreCliente
							 inner join  dbo.PedidoCliente d ON d.IDCliente = c.ID
							 ) as V
	 unpivot ( RosaPedido FOR [NameTipoRosa] IN([Rosas],[Claveles],[Macetas],[Tierra],[Girasoles],[Hortensia],[Globos],[Tarjetas],[Orquideas],[Carmensa],[Lirios],[Aurora],[Tulipanes],[Liston])) upvt	 
	 where RosaPedido&gt;=1
	group by  IDVisita,NameTipoRosa ,RosaPedido
	order by IDVisita  asc</property>
